{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'SearchComponent',\n  data() {\n    return {\n      query: '',\n      summary: '',\n      summaryPoints: [],\n      error: '',\n      loading: false\n    };\n  },\n  methods: {\n    async search() {\n      if (!this.query.trim()) {\n        this.error = 'Please enter a search query.';\n        return;\n      }\n      this.loading = true;\n      this.error = '';\n      this.summary = '';\n      try {\n        console.log('Sending search request for:', this.query); // CHANGE HERE: Logging the search query\n\n        const response = await axios.post('http://localhost:3000/search', {\n          query: this.query\n        });\n        console.log('Received response:', response.data); // CHANGE HERE: Logging the received response\n\n        this.summary = response.data.summary;\n        this.processSummary();\n      } catch (error) {\n        console.error('Error:', error); // CHANGE HERE: Logging the error\n\n        this.error = error.response ? error.response.data.details : error.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n    processSummary() {\n      const points = this.summary.split(/\\d+\\./).filter(point => point.trim() !== '');\n      this.summaryPoints = points.map(point => {\n        const [text, source] = point.split('(Source:');\n        return {\n          text: text.trim(),\n          source: source ? source.replace(')', '').trim() : null\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","query","summary","summaryPoints","error","loading","methods","search","trim","console","log","response","post","processSummary","details","message","points","split","filter","point","map","text","source","replace"],"sources":["/home/kabir/code/madebyDeepseek/ai-search-engine/src/components/Search.vue"],"sourcesContent":["<template>\n  <div class=\"search-container\">\n    <input v-model=\"query\" @keyup.enter=\"search\" placeholder=\"Enter your search query\" class=\"search-input\">\n    <button @click=\"search\" class=\"search-button\">Search</button>\n    <div v-if=\"loading\" class=\"loading\">Searching...</div>\n    <div v-if=\"summary\" class=\"summary-container\">\n      <h3>Summary:</h3>\n      <ul>\n        <li v-for=\"(point, index) in summaryPoints\" :key=\"index\">\n          {{ point.text }}\n          <a v-if=\"point.source\" :href=\"point.source\" target=\"_blank\" rel=\"noopener noreferrer\">Source</a>\n        </li>\n      </ul>\n    </div>\n    <div v-if=\"error\" class=\"error-message\">\n      <h3>Error:</h3>\n      <p>{{ error }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'SearchComponent',\n  data() {\n    return {\n      query: '',\n      summary: '',\n      summaryPoints: [],\n      error: '',\n      loading: false\n    }\n  },\n  methods: {\n    async search() {\n      if (!this.query.trim()) {\n        this.error = 'Please enter a search query.';\n        return;\n      }\n\n      this.loading = true;\n      this.error = '';\n      this.summary = '';\n\n      try {\n        console.log('Sending search request for:', this.query); // CHANGE HERE: Logging the search query\n\n        const response = await axios.post('http://localhost:3000/search', { query: this.query });\n        console.log('Received response:', response.data); // CHANGE HERE: Logging the received response\n\n        this.summary = response.data.summary;\n        this.processSummary();\n      } catch (error) {\n        console.error('Error:', error); // CHANGE HERE: Logging the error\n\n        this.error = error.response ? error.response.data.details : error.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n    processSummary() {\n      const points = this.summary.split(/\\d+\\./).filter(point => point.trim() !== '');\n      this.summaryPoints = points.map(point => {\n        const [text, source] = point.split('(Source:');\n        return {\n          text: text.trim(),\n          source: source ? source.replace(')', '').trim() : null\n        };\n      });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.search-container {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.search-input {\n  width: 70%;\n  padding: 10px;\n  font-size: 16px;\n}\n\n.search-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  cursor: pointer;\n}\n\n.summary-container {\n  margin-top: 20px;\n  background-color: #f5f5f5;\n  padding: 15px;\n  border-radius: 5px;\n}\n\n.summary-container h3 {\n  margin-top: 0;\n}\n\n.summary-container ul {\n  padding-left: 20px;\n}\n\n.summary-container li {\n  margin-bottom: 10px;\n}\n\n.summary-container a {\n  color: #4CAF50;\n  text-decoration: none;\n  margin-left: 5px;\n}\n\n.summary-container a:hover {\n  text-decoration: underline;\n}\n\n.error-message {\n  color: red;\n  margin-top: 20px;\n}\n\n.loading {\n  margin-top: 20px;\n  font-style: italic;\n}\n</style>"],"mappings":"AAsBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,EAAE;MACjBC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACN,KAAK,CAACO,IAAI,CAAC,CAAC,EAAE;QACtB,IAAI,CAACJ,KAAI,GAAI,8BAA8B;QAC3C;MACF;MAEA,IAAI,CAACC,OAAM,GAAI,IAAI;MACnB,IAAI,CAACD,KAAI,GAAI,EAAE;MACf,IAAI,CAACF,OAAM,GAAI,EAAE;MAEjB,IAAI;QACFO,OAAO,CAACC,GAAG,CAAC,6BAA6B,EAAE,IAAI,CAACT,KAAK,CAAC,EAAE;;QAExD,MAAMU,QAAO,GAAI,MAAMb,KAAK,CAACc,IAAI,CAAC,8BAA8B,EAAE;UAAEX,KAAK,EAAE,IAAI,CAACA;QAAM,CAAC,CAAC;QACxFQ,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEC,QAAQ,CAACX,IAAI,CAAC,EAAE;;QAElD,IAAI,CAACE,OAAM,GAAIS,QAAQ,CAACX,IAAI,CAACE,OAAO;QACpC,IAAI,CAACW,cAAc,CAAC,CAAC;MACvB,EAAE,OAAOT,KAAK,EAAE;QACdK,OAAO,CAACL,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC,EAAE;;QAEhC,IAAI,CAACA,KAAI,GAAIA,KAAK,CAACO,QAAO,GAAIP,KA