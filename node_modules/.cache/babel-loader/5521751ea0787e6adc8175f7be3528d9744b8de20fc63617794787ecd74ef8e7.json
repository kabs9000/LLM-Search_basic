{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'SearchComponent',\n  data() {\n    return {\n      query: '',\n      summary: '',\n      searchResults: [],\n      error: ''\n    };\n  },\n  methods: {\n    async search() {\n      try {\n        const response = await axios.post('http://localhost:3000/search', {\n          query: this.query\n        });\n        this.summary = response.data.summary;\n        this.searchResults = response.data.searchResults;\n        this.error = '';\n      } catch (error) {\n        console.error('Error:', error);\n        this.error = error.response ? error.response.data.details : error.message;\n        this.summary = '';\n        this.searchResults = [];\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","query","summary","searchResults","error","methods","search","response","post","console","details","message"],"sources":["/home/kabir/code/madebyDeepseek/ai-search-engine/src/components/Search.vue"],"sourcesContent":["<template>\n    <div class=\"search-container\">\n      <input v-model=\"query\" @keyup.enter=\"search\" placeholder=\"Enter your search query\" class=\"search-input\">\n      <button @click=\"search\" class=\"search-button\">Search</button>\n      <div v-if=\"summary\" class=\"results-container\">\n        <h3>Summary:</h3>\n        <p>{{ summary }}</p>\n        <h3>Search Results:</h3>\n        <ul>\n          <li v-for=\"(result, index) in searchResults\" :key=\"index\">\n            <a :href=\"result.link\" target=\"_blank\">{{ result.title }}</a>\n          </li>\n        </ul>\n      </div>\n      <div v-if=\"error\" class=\"error-message\">\n        <h3>Error:</h3>\n        <p>{{ error }}</p>\n      </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'SearchComponent',\n  data() {\n    return {\n      query: '',\n      summary: '',\n      searchResults: [],\n      error: ''\n    }\n  },\n  methods: {\n    async search() {\n      try {\n        const response = await axios.post('http://localhost:3000/search', { query: this.query });\n        this.summary = response.data.summary;\n        this.searchResults = response.data.searchResults;\n        this.error = '';\n      } catch (error) {\n        console.error('Error:', error);\n        this.error = error.response ? error.response.data.details : error.message;\n        this.summary = '';\n        this.searchResults = [];\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.search-container {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.search-input {\n  width: 70%;\n  padding: 10px;\n  font-size: 16px;\n}\n\n.search-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  cursor: pointer;\n}\n\n.results-container {\n  margin-top: 20px;\n}\n\n.error-message {\n  color: red;\n  margin-top: 20px;\n}\n</style>"],"mappings":"AAsBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,EAAE;MACjBC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAG;MACb,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,8BAA8B,EAAE;UAAEP,KAAK,EAAE,IAAI,CAACA;QAAM,CAAC,CAAC;QACxF,IAAI,CAACC,OAAM,GAAIK,QAAQ,CAACP,IAAI,CAACE,OAAO;QACpC,IAAI,CAACC,aAAY,GAAII,QAAQ,CAACP,IAAI,CAACG,aAAa;QAChD,IAAI,CAACC,KAAI,GAAI,EAAE;MACjB,EAAE,OAAOA,KAAK,EAAE;QACdK,OAAO,CAACL,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;QAC9B,IAAI,CAACA,KAAI,GAAIA,KAAK,CAACG,QAAO,GAAIH,KAAK,CAACG,QAAQ,CAACP,IAAI,CAACU,OAAM,GAAIN,KAAK,CAACO,OAAO;QACzE,IAAI,CAACT,OAAM,GAAI,EAAE;QACjB,IAAI,CAACC,aAAY,GAAI,EAAE;MACzB;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}