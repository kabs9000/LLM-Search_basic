{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport axios from 'axios';\nexport default {\n  name: 'SearchComponent',\n  data() {\n    return {\n      query: '',\n      summary: '',\n      summaryPoints: [],\n      error: '',\n      loading: false\n    };\n  },\n  methods: {\n    async search() {\n      if (!this.query.trim()) {\n        this.error = 'Please enter a search query.';\n        return;\n      }\n      this.loading = true;\n      this.error = '';\n      this.summary = '';\n      try {\n        console.log('Sending search request for:', this.query); // CHANGE HERE: Logging the search query\n\n        const response = await axios.post('http://localhost:3000/search', {\n          query: this.query\n        });\n        console.log('Received response:', response.data); // CHANGE HERE: Logging the received response\n\n        this.summary = response.data.summary;\n        this.processSummary();\n      } catch (error) {\n        console.error('Error:', error); // CHANGE HERE: Logging the error\n\n        this.error = error.response ? error.response.data.details : error.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n    processSummary() {\n      const points = this.summary.split(/\\d+\\./).filter(point => point.trim() !== '');\n      this.summaryPoints = points.map(point => {\n        const text = point.trim();\n        const urlMatch = text.match(/\\bhttps?:\\/\\/\\S+/gi);\n        const source = urlMatch ? urlMatch[0] : null;\n        const title = source ? text.split(source)[0].trim() : text;\n        return {\n          title: title,\n          text: text.replace(title, '').trim(),\n          source: source,\n          favicon: source ? `https://www.google.com/s2/favicons?domain=${new URL(source).hostname}&sz=32` : null\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","query","summary","summaryPoints","error","loading","methods","search","trim","console","log","response","post","processSummary","details","message","points","split","filter","point","map","text","urlMatch","match","source","title","replace","favicon","URL","hostname"],"sources":["/home/kabir/code/madebyDeepseek/ai-search-engine/src/components/Search.vue"],"sourcesContent":["<template>\n  <div class=\"search-container\">\n    <input v-model=\"query\" @keyup.enter=\"search\" placeholder=\"Enter your search query\" class=\"search-input\">\n    <button @click=\"search\" class=\"search-button\">Search</button>\n    <div v-if=\"loading\" class=\"loading\">Searching...</div>\n    <div v-if=\"summary\" class=\"summary-container\">\n      <h3>Summary:</h3>\n      <ul>\n        <li v-for=\"(point, index) in summaryPoints\" :key=\"index\" class=\"summary-point\">\n          <img v-if=\"point.favicon\" :src=\"point.favicon\" :alt=\"point.title\" class=\"favicon\">\n          <div class=\"point-content\">\n            <a v-if=\"point.source\" :href=\"point.source\" target=\"_blank\" rel=\"noopener noreferrer\">{{ point.title }}</a>\n            <strong v-else>{{ point.title }}</strong>\n            <p>{{ point.text }}</p>\n          </div>\n        </li>\n      </ul>\n    </div>\n    <div v-if=\"error\" class=\"error-message\">\n      <h3>Error:</h3>\n      <p>{{ error }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'SearchComponent',\n  data() {\n    return {\n      query: '',\n      summary: '',\n      summaryPoints: [],\n      error: '',\n      loading: false\n    }\n  },\n  methods: {\n    async search() {\n      if (!this.query.trim()) {\n        this.error = 'Please enter a search query.';\n        return;\n      }\n\n      this.loading = true;\n      this.error = '';\n      this.summary = '';\n\n      try {\n        console.log('Sending search request for:', this.query); // CHANGE HERE: Logging the search query\n\n        const response = await axios.post('http://localhost:3000/search', { query: this.query });\n        console.log('Received response:', response.data); // CHANGE HERE: Logging the received response\n\n        this.summary = response.data.summary;\n        this.processSummary();\n      } catch (error) {\n        console.error('Error:', error); // CHANGE HERE: Logging the error\n\n        this.error = error.response ? error.response.data.details : error.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n    processSummary() {\n      const points = this.summary.split(/\\d+\\./).filter(point => point.trim() !== '');\n      this.summaryPoints = points.map(point => {\n        const text = point.trim();\n        const urlMatch = text.match(/\\bhttps?:\\/\\/\\S+/gi);\n        const source = urlMatch ? urlMatch[0] : null;\n        const title = source ? text.split(source)[0].trim() : text;\n        return {\n          title: title,\n          text: text.replace(title, '').trim(),\n          source: source,\n          favicon: source ? `https://www.google.com/s2/favicons?domain=${new URL(source).hostname}&sz=32` : null\n        };\n      });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.search-container {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.search-input {\n  width: 70%;\n  padding: 10px;\n  font-size: 16px;\n}\n\n.search-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  cursor: pointer;\n}\n\n.summary-container {\n  margin-top: 20px;\n  background-color: #f5f5f5;\n  padding: 15px;\n  border-radius: 5px;\n}\n\n.summary-container h3 {\n  margin-top: 0;\n}\n\n.summary-container ul {\n  padding-left: 0;\n  list-style-type: none;\n}\n\n.summary-point {\n  display: flex;\n  align-items: flex-start;\n  margin-bottom: 15px;\n}\n\n.favicon {\n  width: 32px;\n  height: 32px;\n  margin-right: 10px;\n}\n\n.point-content {\n